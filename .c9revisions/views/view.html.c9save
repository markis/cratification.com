{"ts":1343601016196,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"<html>\n<head>\n<title>Cratification</title>\n<style type=\"text/css\">\nbody,html {\n    padding: 0px;\n    margin: 0px;\n    overflow: hidden;\n}\n#container {\n    width: 1024px; \n    height: 300px; \n    border: 1px solid black;\n    margin: auto;\n    overflow: hidden;\n}\n</style>\n</head>\n<body>\n\n<div id=\"container\">\n    <span id=\"ampersand\">&amp;</span>\n</div>\n\n<script type=\"text/javascript\" src=\"http://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js\"></script>\n<script type=\"text/javascript\" src=\"http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.21/jquery-ui.min.js\"></script>\n<script type=\"text/javascript\" src=\"/js/cratify.js\"></script>\n\n<script type=\"text/javascript\">\n$(function() {\n    function loadHash() {\n        if (window.location.hash) {\n            var $container = $(\"#container\");\n            var $ampersand = $(\"#ampersand\");\n            \n            $container.css({height: $(window).height(), width: $(window).width() });\n            \n            var id = window.location.hash.replace('#', '');\n            $.getJSON('/animation/' + id, function (data) { cratify(data, $container, $ampersand); });\n        }\n    }\n    loadHash();\n    $(window).bind('hashchange', loadHash);\n});\n</script>\n\n</body>\n</html>"]],"start1":0,"start2":0,"length1":0,"length2":1226}]],"length":1226}
